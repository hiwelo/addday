{
  "name": "addday",
  "main": "node_modules/expo/AppEntry.js",
  "version": "1.4.0",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "lint": "tsc --noEmit && eslint --ext .ts,.tsx .",
    "lint:fix": "yarn lint --fix",
    "release": "yarn semantic-release",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest"
  },
  "dependencies": {
    "@eva-design/eva": "^1.4.0",
    "@react-native-community/datetimepicker": "2.2.2",
    "@react-native-community/masked-view": "0.1.6",
    "@react-navigation/bottom-tabs": "^5.2.6",
    "@react-navigation/native": "^5.1.3",
    "@react-navigation/stack": "^5.2.8",
    "@ui-kitten/components": "^4.4.1",
    "@ui-kitten/eva-icons": "^5.0.0-alpha.1",
    "crypto-js": "^3.1.9-1",
    "expo": "^37.0.0",
    "expo-device": "~2.1.0",
    "expo-file-system": "~8.1.0",
    "expo-local-authentication": "~9.0.0",
    "expo-localization": "~8.1.0",
    "expo-random": "~8.1.0",
    "expo-secure-store": "~8.1.0",
    "formik": "^2.1.4",
    "i18n-js": "^3.5.1",
    "immutable": "^4.0.0-rc.12",
    "moment": "^2.25.1",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-37.0.0.tar.gz",
    "react-native-appearance": "~0.3.3",
    "react-native-dotenv": "^0.2.0",
    "react-native-gesture-handler": "~1.6.0",
    "react-native-reanimated": "~1.7.0",
    "react-native-safe-area-context": "0.7.3",
    "react-native-screens": "~2.2.0",
    "react-native-svg": "11.0.1",
    "react-native-uuid": "^1.4.9",
    "react-native-web": "^0.11.7",
    "react-redux": "^7.1.3",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "redux-persist-expo-filesystem": "^2.0.0",
    "redux-persist-expo-securestore": "^2.0.0",
    "redux-persist-transform-encrypt": "^2.0.1",
    "redux-persist-transform-immutable": "^5.0.0",
    "reselect": "^4.0.0",
    "sentry-expo": "^2.0.3",
    "styled-components": "^5.0.1",
    "uuid": "^7.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.0.5",
    "@semantic-release/npm": "^7.0.5",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "@types/crypto-js": "^3.1.43",
    "@types/i18n-js": "^3.0.1",
    "@types/jest": "^24.0.25",
    "@types/node": "^13.9.5",
    "@types/react": "^16.9.11",
    "@types/react-native": "^0.60.22",
    "@types/react-native-dotenv": "^0.2.0",
    "@types/react-native-uuid": "^1.4.0",
    "@types/react-redux": "^7.1.7",
    "@types/redux-persist-transform-encrypt": "^2.0.1",
    "@types/styled-components": "^5.0.1",
    "@types/uuid": "^7.0.2",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "babel-plugin-inline-import": "^3.0.0",
    "babel-preset-expo": "^8.1.0",
    "cz-conventional-changelog": "3.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-universe": "^2.3.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "husky": "^4.2.1",
    "i": "^0.3.6",
    "jest-expo": "^37.0.0",
    "npm": "^6.13.7",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "semantic-release": "^17.0.7",
    "semantic-release-expo": "^2.2.3",
    "typescript": "^3.8.3"
  },
  "resolutions": {
    "acorn": "^7.1.1",
    "minimist": "^0.2.1"
  },
  "private": true,
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|react-clone-referenced-element|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|sentry-expo|native-base|redux-persist-expo-securestore))"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "pretty-quick --staged && yarn lint"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "release": {
    "branches": [
      "develop"
    ],
    "verifyConditions": [
      "semantic-release-expo",
      "@semantic-release/changelog",
      "@semantic-release/git",
      "@semantic-release/github",
      "@semantic-release/npm"
    ],
    "generateNotes": [
      "@semantic-release/release-notes-generator"
    ],
    "prepare": [
      "semantic-release-expo",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      {
        "path": "@semantic-release/git",
        "assets": [
          "CHANGELOG.md",
          "package.json",
          "package-lock.json",
          "app.json"
        ]
      }
    ],
    "publish": [
      "@semantic-release/github"
    ],
    "success": false,
    "fail": false
  }
}
